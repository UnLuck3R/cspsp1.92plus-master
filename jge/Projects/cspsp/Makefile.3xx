INCDIR = ../../JGE/include ../../JGE/include/psp ../../JGE/include/psp/freetype2 C:/pspsdk/psp/sdk/include
LIBDIR = ../../JGE/lib/psp
LDFLAGS =
LIBS=-L../../JGE/lib/psp -lhgetools -ljge300 -lfreetype -ljpeg -lgif -lpng -lz -lm -lmad -lmikmod -lmmio -lpsppower -lpspmpeg -lpspaudiocodec -lpspaudiolib -lpspaudio -lpspgum -lpspgu -lpsprtc -lstdc++ -lpspwlan -lpspopenpsid

